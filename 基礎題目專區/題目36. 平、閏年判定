#include <iostream>
#include <iomanip>
#include <bits/stdc++.h>
using namespace std;

int main(){
    int x1, x2, y1, y2, i;
    string str, t;
    double y=0;
    
    while(cin >> x1){
        if(x1 % 4 == 0){
            if(x1 % 100 == 0){
                if(x1 % 400 == 0){
                    cout << "Bissextile Year" << endl;
                }
                else{
                    cout << "Common Year" << endl;
                }
            }
            else{
                cout << "Bissextile Year" << endl;
            }
        }
        else{
            cout << "Common Year" << endl;
        }
        
    }
    return 0;
}
